{
  "name": "oh-my-grid",
  "version": "0.0.1",
  "description": "Graph-based decision-making in case of power grid failures and certain humer error probability conditions.",
  "main": "index.js",
  "scripts": {
    "test": "standard --verbose | snazzy && npm run test:cov",
    "start": "node index.js",
    "start/dev": "nodemon index.js",
    "start/prod": "pm2 apps.js",
    "style/watch": "",
    "seed": "",
    ".test": "standard --verbose | snazzy && npm run db:refresh-test && npm run test:cov",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "coverage/api/open": "coverage/lcov-report/index.html",
    "test:cov": "cross-env NODE_PATH=. NODE_ENV=test istanbul cover node_modules/.bin/mocha -- --exit",
    "test:specs": "cross-env NODE_PATH=. NODE_ENV=test mocha --exit",
    "docs/client": "jsdoc -d docs/client -r src/client/src -c docs.client.conf.json",
    "docs/client/open": "open docs/client/index.html",
    "docs/api": "jsdoc -d docs/api -r src/app src/container src/domain src/infra src/inter -c docs.api.conf.json",
    "docs/api/open": "open docs/api/index.html",
    "analysis/client": "",
    "analysis/client/open": "",
    "analysis/api": "./node_modules/.bin/es6-plato -d reports/api src/app src/domain src/inter/**/**/*.js src/infra/**/**/*.js",
    "analysis/api/open": "open reports/api/index.html",
    "test/unit": "",
    "test/unit/watch": "npm run test --save",
    "test/e2e": "",
    "test/aat": "",
    "test/api": "npm run test",
    "help": "npm run help/nohup",
    "help/open": "npm run $(npm run | percol)",
    "install/help/nohup": "nohup $(npm run)",
    "help/nohup": "npm run $(cat nohup.out | percol)"
  },
  "keywords": [],
  "author": "Aha Hah (aaron.alan.alexander@protonmail.com)",
  "license": "WTFPL",
  "dependencies": {
    "awilix": "^4.3.3",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "casl": "^1.1.0",
    "chai": "^4.3.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "express-status-monitor": "^1.3.3",
    "firebase": "^8.2.9",
    "firebase-admin": "^9.5.0",
    "http-status": "^1.5.0",
    "istanbul": "^0.4.5",
    "mocha": "^8.3.0",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pryjs": "^1.0.3",
    "ramda": "^0.27.1",
    "supertest": "^6.1.3",
    "swagger-jsdoc": "^3.2.9",
    "tcomb": "^3.2.29",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "wolkenkit": "^3.1.2"
  },
  "devDependencies": {
    "better-docs": "^2.3.2",
    "es6-plato": "^1.2.3"
  }
}
